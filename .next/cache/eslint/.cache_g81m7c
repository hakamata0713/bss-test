xt>\r\n        </Stack>\r\n\r\n        <Heading size='md' mt={20} mb={5} pl={2.5} borderLeft=\"solid 5px #0075c2\">会社情報</Heading>\r\n        <TableContainer\r\n          sx={{\r\n            '&::-webkit-scrollbar': {\r\n              width: '16px',\r\n              borderRadius: '16px',\r\n              backgroundColor: `rgba(0, 0, 0, 0.25)`,\r\n            },\r\n            '&::-webkit-scrollbar-thumb': {\r\n              borderRadius: '16px',\r\n              backgroundColor: `rgba(0, 0, 0, 0.25)`,\r\n            },\r\n          }}\r\n        >\r\n          <Table \r\n            variant='simple' \r\n            colorScheme='telegram'\r\n            size='lg'\r\n          >\r\n            <Tbody>\r\n              <Tr>\r\n                <Th>会社名</Th>\r\n                <Td>株式会社ビー・エス・サイトー</Td>\r\n              </Tr>\r\n              <Tr>\r\n                <Th>創業年月日</Th>\r\n                <Td>昭和46年6月1日</Td>\r\n              </Tr>\r\n              <Tr>\r\n                <Th>創立年月日</Th>\r\n                <Td>昭和51年6月1日</Td>\r\n              </Tr>\r\n              <Tr>\r\n                <Th>本社所在地</Th>\r\n                <Td>秋田市川尻新川町9-35</Td>\r\n              </Tr>\r\n              <Tr>\r\n                <Th>代表取締役</Th>\r\n                <Td>柏谷 勝美</Td>\r\n              </Tr>\r\n              <Tr>\r\n                <Th>事業内容</Th>\r\n                <Td>\r\n                  ソフトウェア開発、業務ツール作成、インターネットセキュリティ対策<br/>\r\n                  ソフトウェア開発技術者の派遣<br/>\r\n                  IT助成金の活用支援、コンサルティング<br/> \r\n                  パソコン、パッケージソフトの販売<br/> \r\n                  文具事務用品、スチール家具、コピー機の販売\r\n                </Td>\r\n              </Tr>\r\n              <Tr>\r\n                <Th>営業エリア</Th>\r\n                <Td>秋田県、東北各県</Td>\r\n              </Tr>\r\n              <Tr>\r\n                <Th>資本金</Th>\r\n                <Td>15,000千円</Td>\r\n              </Tr>\r\n              <Tr>\r\n                <Th>社員数</Th>\r\n                <Td>24名</Td>\r\n              </Tr>\r\n  